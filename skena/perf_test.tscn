	server.pemain[\"pengamat\"][\"posisi\"] = get_node(\"../../../viewport_utama/viewport/pengamat\").position
	server.pemain[\"pengamat\"][\"rotasi\"] = get_node(\"../../../viewport_utama/viewport/pengamat\").rotation
"

