script/source = "# 28/12/23
extends Control

# 27/07/24 :: hasil benchmark pre rendering
# * dapatkan jumlah fps ketika memulai pre rendering
# * dapatkan jumlah vertex ketika memulai pre rendering
# * kalkulasi total jumlah objek yang dirender
# * kalkulasi total waktu (milidetik) proses rendering satu objek
# * kalkulasi total waktu (milidetik) proses rendering satu vertex
# * hasilkan grafik jumlah vertex tiap objek yang di-render
# misal 60 fps, maka satu frame adalah 16.666666666666 milidetik atau 1000/60
# jika jumlah objek adalah 213 maka total waktu rendering per objek adalah 0.0782472613458 milidetik atau 16.666666666666/213
# jika jumlah vertex adalah 104840 maka total waktu rendering per vertex adalah 0.000161055951746779 milidetik atau 16.666666666666/104840

var shader_tampilan_bentuk_objek : StandardMaterial3D = load(\"res://material/dev/tampilan_bentuk_objek.material\")
var shader_tampilan_bentuk_objek_lod : StandardMaterial3D = load(\"res://material/dev/tampilan_bentuk_objek_lod.material\")
var _proses_pre_render : bool = false :
	set(proses):
		if proses:	$mode_render/realtime.disabled = true
		else: 		$mode_render/realtime.disabled = false
		_proses_pre_render = proses
var _pencahayaan_pre : Environment
var waktu_tunda_frame_pre : float = 0
var waktu_tunda_frame_pre_u : float = 0.05
var waktu_tunda_frame_pre_o : float = 0.1

func _ready():
	server.permainan.karakter = $viewport_utama/viewport/dummy_karakter
	server.permainan.dunia.pengamat = $viewport_utama/viewport/pengamat
	if server.permainan.permukaan != null: server.permainan.permukaan.pengamat = $viewport_utama/viewport/pengamat
	_pencahayaan_pre = $viewport_utama/viewport/pengamat_pre.environment
	server.pemain[\"pengamat\"] = {
		\"id_client\": 1,
		\"nama\": 	\"\",
		\"posisi\":	$viewport_utama/viewport/pengamat.position,
		\"rotasi\":	$viewport_utama/viewport/pengamat.rotation,
		\"model\": { },
		\"kondisi\": { }
	}

func atur_posisi(posisi_x : float, posisi_y : float, posisi_z : float):
	$viewport_utama/viewport/pengamat.global_position.x = posisi_x
	$viewport_utama/viewport/pengamat.global_position.y = posisi_y
	$viewport_utama/viewport/pengamat.global_position.z = posisi_z

# ketika mengatur culling
func _atur_frustum_culling(nilai):
	_mode_render_realtime()
	server.permainan.gunakan_frustum_culling = nilai
func _atur_object_culling(nilai):
	_mode_render_realtime()
	server.permainan.gunakan_occlusion_culling = nilai
func _atur_level_of_detail(nilai):
	_mode_render_realtime()
	#$viewport_utama/viewport/lingkungan/permukaan.gunakan_level_of_detail = nilai

# ketika memilih mode rendering
func _mode_render_realtime():
	if not _proses_pre_render:
		# sembunyikan hasil performa rendering
		$panel_hasil_render.visible = false
		# set keadaan tombol
		$mode_render/realtime.button_pressed = true
		$mode_render/pre.button_pressed = false
		# tukar pengamat
		$viewport_utama/viewport/pengamat.current = true
		# sembunyikan proses rendering
		$panel_bawah/animasi.stop()
		$panel_bawah/Panel/SpinnerMemuat/AnimationPlayer.stop()
		$panel_bawah.visible = false
		# tampilkan informasi performa
		$informasi_performa.visible = true
		#region reset fase rendering
		# 1. jalankan proses culling
		server.set_process(true)
		server.permainan.dunia.set_process(true)
		# 2. tampilkan kembali lingkungan
		server.permainan.dunia.get_node(\"lingkungan\").set(\"visible\", true)
		#endregion
func _mode_render_pre():
	if not _proses_pre_render:
		_proses_pre_render = true
		# set keadaan tombol
		$mode_render/realtime.button_pressed = false
		$mode_render/pre.button_pressed = true
		# set timing mode optimasi
		if $panel_opsi/opsi_fc.button_pressed:
			waktu_tunda_frame_pre = waktu_tunda_frame_pre_o
		else:
			waktu_tunda_frame_pre = waktu_tunda_frame_pre_u
		# tukar pengamat
		$viewport_utama/viewport/pengamat_pre.current = true
		$viewport_utama/viewport/pengamat_pre.fov = $viewport_utama/viewport/pengamat.fov
		$viewport_utama/viewport/pengamat_pre.position = $viewport_utama/viewport/pengamat.position
		$viewport_utama/viewport/pengamat_pre.rotation = $viewport_utama/viewport/pengamat.rotation
		# sembunyikan informasi performa
		$informasi_performa.visible = false
		$panel_hasil_render.visible = false
		# tampilkan proses rendering
		$panel_bawah/Panel/PersenMemuat.text = \"0%\"
		$panel_bawah/Panel/ProsesMemuat.value = 0
		$panel_bawah/animasi.play(\"tampilkan\")
		$panel_bawah/Panel/SpinnerMemuat/AnimationPlayer.play(\"kuru_kuru\")
		# dapatkan performa rendering
		var jumlah_penggambaran = RenderingServer.get_rendering_info(RenderingServer.RENDERING_INFO_TOTAL_DRAW_CALLS_IN_FRAME)
		var kecepatan_frame = Engine.get_frames_per_second()
		var total_objek = server.permainan.dunia.get_node(\"objek\").get_child_count()
		var total_sudut = $viewport_utama/viewport.get_render_info(Viewport.RENDER_INFO_TYPE_VISIBLE, Viewport.RENDER_INFO_PRIMITIVES_IN_FRAME)
		var total_memori = RenderingServer.get_rendering_info(RenderingServer.RENDERING_INFO_VIDEO_MEM_USED)
		#region tampilan fase rendering
		# 1. hentikan proses culling
		server.set_process(false)
		server.permainan.dunia.set_process(false)
		# 2. sembuyikan lingkungan
		server.permainan.dunia.get_node(\"lingkungan\").set(\"visible\", false)
		# 3. sembunyikan pencahayaan
		var aktifkan_pencahayaan = false
		if server.permainan.dunia.get_node(\"matahari\").shadow_enabled:
			server.permainan.dunia.get_node(\"matahari\").set(\"shadow_enabled\", false)
			aktifkan_pencahayaan = true
		$viewport_utama/viewport/pengamat_pre.environment = _pencahayaan_pre
		# 4. dapatkan objek yang nampak kemudian tambahkan ke array
		var daftar_objek : Dictionary
		for objek_ in server.permainan.dunia.get_node(\"objek\").get_children():
			if objek_.visible:
				var jarak_objek = objek_.global_position.distance_to($viewport_utama/viewport/pengamat_pre.global_position)
				daftar_objek[jarak_objek] = objek_
		# 5. sembunyikan semua objek yang nampak
		for _objek in daftar_objek: daftar_objek[_objek].set(\"visible\", false)
		# 6. set max_value dari $panel_bawah/Panel/ProsesMemuat dengan jumlah objek yang nampak
		$panel_bawah/Panel/ProsesMemuat.max_value = daftar_objek.size()
		# 7. dapatkan indeks daftar objek
		var indeks_daftar_objek = daftar_objek.keys()
		# 8. buat urutan objek
		var urutan_objek : Array = []
		for _objek in daftar_objek: urutan_objek.append(_objek)
		# 9. sesuaikan indeks urutan objek
		urutan_objek.sort()
		# 10. balik urutan objek
		urutan_objek.reverse()
		await get_tree().create_timer(waktu_tunda_frame_pre).timeout
		# 11. loop urutan objek
		for _objek_ in urutan_objek:
			if $viewport_utama/viewport/pengamat_pre.current:
				# dapatkan objek
				var objek_ = daftar_objek[_objek_]
				# tampilkan objek
				objek_.set(\"visible\", true)
				# tampilkan shader penanda bentuk objek
				for isi_objek in objek_.get_children():
					if isi_objek is MeshInstance3D:
						isi_objek.set(\"material_overlay\", shader_tampilan_bentuk_objek)
					elif isi_objek is Node3D:
						match isi_objek.name:
							\"model\", \"detail\", \"lod_1\":
								for isi_isi_objek in isi_objek.get_children():
									if isi_isi_objek is MeshInstance3D:
										isi_isi_objek.set(\"material_overlay\", shader_tampilan_bentuk_objek)
							\"lod_2\":
								for isi_isi_objek in isi_objek.get_children():
									if isi_isi_objek is MeshInstance3D:
										isi_isi_objek.set(\"material_overlay\", shader_tampilan_bentuk_objek_lod)
				await get_tree().create_timer(waktu_tunda_frame_pre).timeout
				# sesuaikan persentase proses rendering
				$panel_bawah/Panel/ProsesMemuat.value += 1
				$panel_bawah/Panel/PersenMemuat.text = str(($panel_bawah/Panel/ProsesMemuat.value / urutan_objek.size()) * 100)+\"%\"
				# hapus shader penanda bentuk objek
				for isi_objek in objek_.get_children():
					if isi_objek is MeshInstance3D:
						isi_objek.set(\"material_overlay\", null)
					elif isi_objek is Node3D:
						match isi_objek.name:
							\"model\", \"detail\", \"lod_1\":
								for isi_isi_objek in isi_objek.get_children():
									if isi_isi_objek is MeshInstance3D:
										isi_isi_objek.set(\"material_overlay\", null)
							\"lod_2\":
								for isi_isi_objek in isi_objek.get_children():
									if isi_isi_objek is MeshInstance3D:
										isi_isi_objek.set(\"material_overlay\", null)
			else: return
		# 12. tunggu selama waktu_tunda_frame_pre detik
		await get_tree().create_timer(waktu_tunda_frame_pre).timeout
		# 13. tampilkan kembali pencahayaan
		$viewport_utama/viewport/pengamat_pre.environment = null
		if aktifkan_pencahayaan:
			server.permainan.dunia.get_node(\"matahari\").set(\"shadow_enabled\", true)
		await get_tree().create_timer(waktu_tunda_frame_pre).timeout
		# 14. tampilkan kembali lingkungan
		server.permainan.dunia.get_node(\"lingkungan\").set(\"visible\", true)
		#endregion
		# sembunyikan proses rendering
		$panel_bawah/animasi.play_backwards(\"tampilkan\")
		$panel_bawah/Panel/SpinnerMemuat/AnimationPlayer.stop()
		# kalkulasi hasil performa rendering
		var hasil_performa = \"\"
		var waktu_frame : float = 1000 / kecepatan_frame
		hasil_performa += \"Penggambaran			: \"+str(jumlah_penggambaran)+\" / \"+str(snappedf(waktu_frame / jumlah_penggambaran, 0.00001))+\" milidetik\\n\"
		hasil_performa += \"Kecepatan Frame		: \"+str(kecepatan_frame)+\" per detik / \"+str(snappedf(waktu_frame, 0.00001))+\" milidetik\\n\"
		hasil_performa += \"Total Objek				: \"+str(total_objek)+\" per frame / \"+str(snappedf(waktu_frame / total_objek, 0.0001))+\" milidetik\\n\"
		hasil_performa += \"Total Sudut				: \"+str(total_sudut)+\" per frame / \"+str(snappedf(waktu_frame / total_sudut, 0.0001))+\" milidetik\\n\"
		hasil_performa += \"Total Memori				: \"+String.humanize_size(total_memori)
		# tampilkan hasil performa rendering
		$panel_hasil_render/statistik.text = hasil_performa
		$panel_hasil_render.visible = true
		_proses_pre_render = false

# ketika memilih tampilan viewport culling
func _pilih_tidak_ada():
	$panel_opsi/pilih_fc.button_pressed = false
	$panel_opsi/pilih_oc.button_pressed = false
	$panel_opsi/pilih_lod.button_pressed = false
	$panel_opsi/pilih_mipmap.button_pressed = false
func _pilih_frustum_culling():
	_pilih_tidak_ada()
	$panel_opsi/pilih_fc.button_pressed = true
func _pilih_occlusion_culling():
	_pilih_tidak_ada()
	$panel_opsi/pilih_oc.button_pressed = true
func _pilih_lod():
	_pilih_tidak_ada()
	$panel_opsi/pilih_lod.button_pressed = true
func _pilih_mipmap():
	_pilih_tidak_ada()
	$panel_opsi/pilih_mipmap.button_pressed = true

# ketika mengatur opsi render
func _atur_fov(nilai):
	if nilai > 179: $panel_opsi_2/atur_fov.release_focus(); return
	$viewport_utama/viewport/pengamat.fov = nilai
	$viewport_utama/viewport/pengamat.global_position.y += 0.01
	$viewport_culling/SubViewport/pengamat/tampilan_fov_kiri.rotation_degrees.y = nilai / 2
	$viewport_culling/SubViewport/pengamat/tampilan_fov_kanan.rotation_degrees.y = -(nilai / 2)
func _atur_overdraw(aktif):
	if aktif:
		$viewport_utama/viewport.debug_draw = Viewport.DEBUG_DRAW_OVERDRAW
	else:
		$viewport_utama/viewport.debug_draw = Viewport.DEBUG_DRAW_DISABLED
func _atur_fov_minimap(nilai):
	$viewport_culling/SubViewport/pengamat/Camera3D.size = nilai
"

